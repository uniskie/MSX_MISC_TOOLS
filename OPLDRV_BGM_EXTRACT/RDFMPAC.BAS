40 INPUT"FMPAC SLOT (DEFAULT=1)";S$
50 IF S$="" THEN S=1:GOTO 80
60 S=VAL("&H"+S$):IF S=0 THEN 50
70 POKE AD+3,S
80 COLOR15,0,0:SCREEN5
90 DEFUSR=AD:U=USR(0)
100 FOR I=0 TO 5:READ BA(I):NEXT
110 S=BA(0)'-&H4000
120 E=BA(5)-1'-&H4000
130 BSAVE"FMPACBGM.BIN",S,E,S
140 FOR I=0 TO 4
150  S=BA(I)+2'-&H4000
160  E!=S+VPEEK(S-2)+VPEEK(S-1)*256-1'-&H4000
170  BSAVE"FMPAC"+HEX$(I)+".BIN",S,E!,S
180 NEXT
190 END
