10 H=PEEK(&HFC4A)+PEEK(&HFC4B)*256:PRINT"HIMEM:$";HEX$(H)
20 KEY1,"CLEAR200,&H"+HEX$(H)
30 F$="FW.BIN"
40 OPEN F$ FOR INPUT AS #1:A$=INPUT$(5,#1):CLOSE#1
50 FORJ=0TO1:AD(J)=ASC(MID$(A$,2+J*2,1))+ASC(MID$(A$,3+J*2,1))*256:NEXT
60 PRINT F$+" START:$"+HEX$(AD(0))+" END:$"+HEX$(AD(1))
70 IF H < AD(1) THEN PRINT"END ADDRES OVER HIMEM":I$=INPUT$(1)
80 CLEAR200,AD(0)
90 BLOAD"FW.BIN",R

